{
	"name": "Green Gold",
	"type": "dark",
	"$schema": "vscode://schemas/color-theme",

	"tokenColors": [
		{
			"name": "Primary Color",
			"scope": 
			[
				// Default	s
				"source", 
				"text",

				// Variables
				"variable",
				
				// Function Names, Constructors and Destructors
				"entity.name.function",

				
			],
			"settings": {
				"foreground": "#f2fffcdc"
			}
		},
		
		{
			"name": "Faded Color",
			"scope": [
				
				// Punctuation
				"punctuation",

				// Braces
				"punctuation.section",

				// Access control
				"storage.type.modifier.access.control",
				"storage.type.modifier.access.public.cpp",
				"storage.type.modifier.access.private.cpp",

				// Class and Struct
				"storage.type.class", 
				"storage.type.struct", 
				"storage.type.enum",
				"storage.type.namespace",

				//using
				"keyword.other.using",

				// Preprocessor Directives #define, #include
				"keyword.control.directive",
				"punctuation.definition.directive",
				"meta.preprocessor",

				
				// Operators
				"keyword.operator",
				"entity.name.function.operator.cpp",
				"entity.name.function.operator.member.cpp",
				"storage.modifier.reference.cpp",
				"storage.modifier.pointer.cpp",
			],
			"settings": {
				"foreground": "#ddffff66"
			}
		},
		{
			"name": "Comment Color",
			"scope": [
				// Comments
				"comment",
				"punctuation.definition.comment",
				
			],
			"settings": {
				"foreground": "#9af79d89"
			}
		},
		{
			"name": "Comment Link",
			"scope": [
				// Comments
				"storage.type.class.doxygen.cpp",
				"keyword.other.documentation.javadoc",
				"keyword.other.documentation.doxygen",
			],
			"settings": {
				"foreground": "#9af79dae",
				"fontStyle": "bold"
			}
		},

		{
			"name": "Primary Accent Color",
			"scope": [
				// Control flow keywords
				"keyword.control",

				// Type names
				"entity.name.type", 
				"storage.type.primitive",

				// Namespaces
				"entity.name.namespace",
			],
			"settings": {
				"foreground": "#ffed72"
				
			}
		},
		{
			"name": "Secondary Accent Color",
			"scope": [
				// Numbers
				"constant.numeric",
				// String Literals
				"string.quoted","punctuation.definition.string",
				
				
			],
			"settings": {
				"foreground": "#ffb83c"
			}
		},
		{
			"name": "Tertiary Accent Color",
			"scope": [
				
				// Templates
				"storage.type.template",
				
				// Inheritance, Const, Static, Volatile, Mutable
				"storage.modifier",




			],
			"settings": {
				"foreground": "#538ccd"
			}
		}

	],
	
